{
  "name": "xswoft/security",
  "type": "library",
  "keywords": [
    "swoft",
    "security"
  ],
  "description": "Security component for swoft framework",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "axkeson",
      "email": "axkeson@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "swoft/framework": "~2.0.0",
    "swoft/http-message": "^2.0",
    "swoft/http-server": "^2.0",
    "swoft/swoole-ide-helper": "^4.4",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/simple-cache": "^1.0",
    "firebase/php-jwt": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Xswoft\\Security\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "XswoftTest\\Security\\Testing\\": "test/testing",
      "XswoftTest\\Security\\Unit\\": "test/unit"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
